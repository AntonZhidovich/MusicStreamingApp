{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:7283"
  },
  "Routes": [
    {
      "Priority": 1,
      "UpstreamPathTemplate": "/identity/authorization/{everything}",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/authorization/{everything}",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7031
        }
      ]
    },
    {
      "Priority": 1,
      "UpstreamPathTemplate": "/identity/users",
      "UpstreamHttpMethod": [ "Post" ],
      "DownstreamPathTemplate": "/api/users",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7031
        }
      ]
    },
    {
      "Priority": 1,
      "UpstreamPathTemplate": "/music/songs/sources/{authorName}/{sourceName}",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/songs/sources/{authorName}/{sourceName}",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7127
        }
      ]
    },
    {
      "Priority": 0,
      "UpstreamPathTemplate": "/identity/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7031
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "SwaggerKey": "identity"
    },
    {
      "UpstreamPathTemplate": "/music/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7127
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "SwaggerKey": "musicservice"
    },
    {
      "UpstreamPathTemplate": "/subscription/{everything}",
      "UpstreamHttpMethod": [ "Get", "Post", "Put", "Delete" ],
      "DownstreamPathTemplate": "/api/{everything}",
      "DownstreamScheme": "https",
      "DangerousAcceptAnyServerCertificateValidator": true,
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7060
        }
      ],
      "AuthenticationOptions": {
        "AuthenticationProviderKey": "Bearer"
      },
      "SwaggerKey": "subscriptionservice"
    }
  ],
  "SwaggerEndPoints": [
    {
      "Key": "identity",
      "Config": [
        {
          "Name": "Identity API",
          "Version": "v1",
          "Url": "https://localhost:7031/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "musicservice",
      "Config": [
        {
          "Name": "MusicService API",
          "Version": "v1",
          "Url": "https://localhost:7127/swagger/v1/swagger.json"
        }
      ]
    },
    {
      "Key": "subscriptionservice",
      "Config": [
        {
          "Name": "SucriptionService API",
          "Version": "v1",
          "Url": "https://localhost:7060/swagger/v1/swagger.json"
        }
      ]
    }
  ]
}
